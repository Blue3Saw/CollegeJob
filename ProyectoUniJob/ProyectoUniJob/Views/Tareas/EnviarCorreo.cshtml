@using BO
@model TareasBO


<div class="col-md-2"></div>
<div class="clearfix">&nbsp;</div>
<div class="container-fluid">
    <div class="container" style="width:109%;">
        @using (Ajax.BeginForm("EnviarCorreo", "Tareas", new AjaxOptions() { HttpMethod = "POST" }))
        { 
            <h3>@Model.Titulo</h3>
            <div class="col-md-12 bann-info">
                <div class="row">
                    <button class="btn btn-success right" style="background-color:#46A69B;" type="submit">Finalizar tarea</button>
                </div>
            </div>
        }
    </div>
</div>

